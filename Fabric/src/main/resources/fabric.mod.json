{
  "schemaVersion": 1,
  "id": "quickrightclick",
  "version": "1.0",

  "name": "Quick Right-Click",
  "description": "Adds the functionality to right-click items in the air to access them quickly without placing them. Works for beds, cartography tables, crafting tables, ender chests, grindstones, shulker boxes, smithing tables and stonecutters.",
  "authors": [ "Rick South" ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/quick-right-click",
    "sources": "https://github.com/ricksouth/serilum-mc-mod-sources/tree/main/sources/Quick%20Right-Click",
    "issues": "https://github.com/ricksouth/serilum-mc-mods/labels/Mod:%20Quick%20Right-Click"
  },

  "license": "All Right Reserved",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": { "main": [ "com.natamus.quickrightclick.ModFabric" ], "modmenu": [ "com.natamus.quickrightclick.fabric.config.IntegrateModMenu" ] },
  "mixins": [
    "quickrightclick_fabric.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.5",
    "fabric": "*",
    "minecraft": ">=1.19",
    "java": ">=17",
    "collective": ">=6.62"
  },

